class Solution {
    public int busyStudent(int[] startTime, int[] endTime, int queryTime) {
        int studNum = 0;
        int n = startTime.length;
        for(int i = 0; i<n; i++){
            if (startTime[i] <= queryTime && endTime[i] >= queryTime){
                studNum++;    
            }
        }
        return studNum++; //studNum++ and studeNum same here but ++studNum not same.
    }
}
